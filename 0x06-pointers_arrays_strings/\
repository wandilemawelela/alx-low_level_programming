#include "main.h"

/**
  * *string_toupper - changes all lowercase letters of a string to uppercase
  * @str: Char to be converted
  * Return: 0
  */

char *string_toupper(char *str)
{
	int i;

	while (*str[i] != '\0')
	{
		if (*str[i] >= 'a' && *str[i] <= 'z')
		{
			*str[i] = *str[i] - 32;
		}
		i++;
	}
	return (0);
}
